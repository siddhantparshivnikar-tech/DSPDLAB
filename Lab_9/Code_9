// Assume that you have a seven-slot closed hash table (the slots are numbered o through 6) Write program  to final hash table that would result if you used the hash function h(k) - k mod 7.

#include <stdio.h>
#define SIZE 7
int main() {
    int hash[SIZE];
    int n, key, index, i;
    for (i = 0; i < SIZE; i++)
        hash[i] = -1;
    printf("Enter number of keys to insert: ");
    scanf("%d", &n);
    printf("Enter %d keys:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &key);
        index = key % SIZE;  // hash function
        // Linear probing
        while (hash[index] != -1) {
            index = (index + 1) % SIZE;
        }

        hash[index] = key;
    }
    printf("\nFinal Hash Table:\n");
    for (i = 0; i < SIZE; i++) {
        printf("Slot %d : %d\n", i, hash[i]);
    }
    return 0;
}
